			"",
			"var items = [",
			"    { name: 'Edward', value: 21 },",
			"    { name: 'Sharpe', value: 37 },",
			"    { name: 'And', value: 45 },",
			"    { name: 'The', value: -12 },",
			"    { name: 'Magnetic' },",
			"    { name: 'Zeros', value: 37 }",
			"  ];",
			"  ",
			"  // sort by value",
			"  items.sort(function (a, b) {",
			"    return (a.value - b.value)",
			"  });",
			"  ",
			"  // sort by name",
			"  items.sort(function(a, b) {",
			"    var nameA = a.name.toUpperCase(); // ignore upper and lowercase",
			"    var nameB = b.name.toUpperCase(); // ignore upper and lowercase",
			"    if (nameA < nameB) {",
			"      return -1;",
			"    }",
			"    if (nameA > nameB) {",
			"      return 1;",
			"    }",
			"  ",
			"    ",
			"  // names must be equal",
			"  ",
			"    return 0;",
			"  });"
		],
		"description": "对象排序"
