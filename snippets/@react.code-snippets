			"    <BrowserRouter>",
			"        <Switch>",
			"            <Route path=\"/login\" component={Login} />",
			"            <Route path=\"/register\" component={Register} />",
			"            <Route component={DashBoard} />",
			"        </Switch>",
			"    </BrowserRouter>",
			");",
			"",
			"// index.js内容",
			"const render = Component => {",
